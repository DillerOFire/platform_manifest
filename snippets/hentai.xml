<manifest>
  <remote name="github/hentaiOS"
          fetch="https://github.com/hentaiOS"
          revision="refs/heads/SaltyIceCream" />

  <remote name="gitlab/hentaiOS"
          fetch="https://gitlab.com/hentaiOS"
          revision="refs/heads/SaltyIceCream" />

  <remote name="github/LineageOS"
          fetch="https://github.com/LineageOS"
	  revision="refs/heads/lineage-18.1" />

  <remote  name="arrow"
           fetch="https://github.com/ArrowOS"
           review="https://review.arrowos.net/"
           revision="refs/heads/arrow-12.0" />
  <remote  name="arrow-caf"
           fetch="https://github.com/ArrowOS"
           review="https://review.arrowos.net/"
           revision="refs/heads/arrow-12.0-caf" />

  <!-- ものを削除 -->
  <remove-project name="device/google/bonito" />
  <remove-project name="device/google/bonito-kernel" />
  <remove-project name="device/google/bramble" />
  <remove-project name="device/google/coral" />
  <remove-project name="device/google/coral-kernel" />
  <remove-project name="device/google/crosshatch" />
  <remove-project name="device/google/crosshatch-kernel" />
  <remove-project name="device/google/crosshatch-sepolicy" />
  <remove-project name="device/google/fuchsia" />
  <remove-project name="device/google/redbull" />
  <remove-project name="device/google/redbull-kernel" />
  <remove-project name="device/google/redfin" />
  <remove-project name="device/google/sunfish" />
  <remove-project name="device/google/sunfish-kernel" />
  <remove-project name="device/google/trout" />
  <remove-project name="platform/bootable/recovery" />
  <remove-project name="platform/build" />
  <remove-project name="platform/frameworks/base" />
  <remove-project name="platform/system/core" />
  <remove-project name="platform/hardware/qcom/wlan" />
  <remove-project name="platform/hardware/libhardware" />

  <!-- hentaiOSのスタート -->
  <project path="build/make" name="platform_build" groups="pdk" remote="github/hentaiOS" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="bootable/recovery" name="platform_bootable_recovery" groups="pdk" remote="github/hentaiOS" />
  <project path="external/json-c" name="android_external_json-c" groups="pdk" remote="arrow" />
  <project path="frameworks/base" name="platform_frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="github/hentaiOS" />
  <project path="hardware/qcom/wlan" name="platform_hardware_qcom_wlan" groups="qcom_wlan,pdk-qcom" remote="github/hentaiOS" />
  <project path="hardware/libhardware" name="android_hardware_libhardware" groups="pdk" remote="arrow" />
  <project path="hardware/qcom-caf/common" name="android_hardware_qcom-caf_common" groups="qcom,pdk-qcom" remote="arrow" >
    <!-- add guard for AOSP hardware/qcom dir -->
    <linkfile src="os_pickup_aosp.mk" dest="hardware/qcom/Android.mk" />
    <!-- add guards for CAF repositories -->
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/msm8998/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/msm8998/Android.mk" />
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/sdm845/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sdm845/Android.mk" />
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/sm8150/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8150/Android.mk" />
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/sm8250/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8250/Android.mk" />
  </project>

  <!-- external repos -->
  <project path="external/ant-wireless/ant_client" name="android_external_ant-wireless_ant_client" remote="arrow" />
  <project path="external/ant-wireless/ant_native" name="android_external_ant-wireless_ant_native" remote="arrow" />
  <project path="external/ant-wireless/ant_service" name="android_external_ant-wireless_ant_service" remote="arrow" />
  <project path="external/ant-wireless/antradio-library" name="android_external_ant-wireless_antradio-library" remote="arrow" />
  <project path="external/ant-wireless/hidl" name="android_external_ant-wireless_hidl" remote="arrow" />
  <project path="external/exfatprogs" name="android_external_exfatprogs" remote="arrow" />
  <project path="external/ntfs-3g" name="android_external_ntfs-3g" remote="arrow" />
  <project path="external/tinyxml" name="android_external_tinyxml" remote="arrow" />

  <project path="system/core" name="platform_system_core" groups="pdk" remote="github/hentaiOS" />
  <project path="vendor/codeaurora/telephony" name="android_vendor_codeaurora_telephony" groups="vendor" remote="arrow" />
  <project path="vendor/gms" name="platform_vendor_gms" groups="vendor" remote="gitlab/hentaiOS" clone-depth="1"/>
  <project path="vendor/hentai" name="platform_vendor_hentai" groups="vendor" remote="github/hentaiOS" />
  <project path="vendor/qcom/opensource/data-ipa-cfg-mgr" name="android_vendor_qcom_opensource_data-ipa-cfg-mgr" groups="vendor" remote="arrow" />
  <project path="vendor/qcom/opensource/dataservices" name="android_vendor_qcom_opensource_dataservices" groups="vendor" remote="arrow" />
  <project path="vendor/qcom/opensource/interfaces" name="android_vendor_qcom_opensource_interfaces" groups="vendor" remote="arrow" />
  <project path="vendor/qcom/opensource/power" name="android_vendor_qcom_opensource_power" groups="vendor" remote="arrow" />
  <project path="hardware/qcom-caf/bt" name="android_hardware_qcom_bt" groups="pdk" remote="arrow" revision="arrow-11.0-caf" />
	<!-- <project path="hardware/qcom-caf/wlan" name="android_hardware_qcom-caf_wlan" groups="pdk" remote="arrow" /> -->

  <!-- Additions from LineageOS -->
  <project path="prebuilts/extract-tools" name="android_prebuilts_extract-tools" remote="github/LineageOS" />
  <project path="prebuilts/tools-hentai" name="android_prebuilts_tools-lineage" remote="github/LineageOS" />

</manifest>
